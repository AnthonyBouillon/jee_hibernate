<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Matches database fields -->
<hibernate-mapping>
    <class name="model.Users" table="users">
        <id column="id" name="id" type="java.lang.Integer">
            <generator class="native"/>
        </id>
        <property column="name" name="name" type="java.lang.String" />
        <property column="password" name="password" type="string" />
        <property column="email" name="email" type="java.lang.String" />
        <property column="sex" name="sex" type="java.lang.String" />
        <property column="country" name="country" type="java.lang.String" />
    </class>
</hibernate-mapping>